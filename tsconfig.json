{
	"compilerOptions": {
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"sourceMap": true,
		"target": "ESNext",
		"strict": true,
		"jsx": "preserve",
		"resolveJsonModule": true,
		"typeRoots": ["node_modules/@types", "src"],
		"skipLibCheck": true,
		"lib": ["esnext", "dom"],
		"sourceRoot": "./",
		"baseUrl": "."
	}
}
